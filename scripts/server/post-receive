#!/bin/sh

#https://gist.github.com/noelboss/3fe13927025b89757f8fb12e9066f2fa

echo "Server script started"

echo "git reset devops"
git --work-tree=$HOME/working-tree-project.git reset devops

echo "git checkout -f devops"
git --work-tree=$HOME/working-tree-project.git checkout -f devops

echo "Server script competed"